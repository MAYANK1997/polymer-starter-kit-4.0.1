constructor() {
        this.agGridLicense = null; // Initialize as null to indicate not yet loaded
    }

    async loadAgridLicense() {
        try {
            const agGridConfigUrl = 'http://nycuapp01.biz.lodh.com:9002/mdm/api/list/ag-grid-license';
            const response = await fetch(agGridConfigUrl);
            if (!response.ok) {
                throw new Error(`HTTP error! Status: ${response.status}`);
            }
            const data = await response.json();
            const agGridLicense = data[0]?.licenseInfo;
            if (agGridLicense) {
                this.agGridLicense = agGridLicense;
                console.log('AG Grid License loaded:', agGridLicense);
            } else {
                throw new Error('No license info found in response');
            }
        } catch (error) {
            console.error('Failed to load AG Grid license:', error);
            this.agGridLicense = ''; // Fallback to empty string if fetch fails
        }
    }

    async getSettings() {
        // Wait for the license to be loaded
        if (this.agGridLicense === null) {
            await this.loadAgridLicense();
        }
